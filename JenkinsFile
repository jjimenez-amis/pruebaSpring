pipeline {
	agent any

	environment{
	//DOCKER_BUILDER_USERNAME_HCARGA = "${params.DOCKER_BUILDER_USERNAME_HCARGA}"
	branch = "${params.BRANCH}"
	}
	stages{
		stage("Checkout"){
 			steps{
				//sh 'chmod 777 ./gradlew'
				//sh './gradlew pruebaParametro -DfirstParam=${DOCKER_BUILDER_USERNAME_HCARGA}'
				sh 'git checkout ${branch}'
			}
	        }
		
		stage("Run"){
			steps{
				echo 'BRANCH MASTER'
			}
			
		}
	}

}
